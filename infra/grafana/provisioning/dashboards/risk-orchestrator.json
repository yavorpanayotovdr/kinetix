{
  "uid": "kinetix-risk-orchestrator",
  "title": "Risk Orchestrator",
  "tags": [
    "kinetix",
    "risk-orchestrator"
  ],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "VaR Calculation Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (calculationType) (rate(var_calculation_count_total{job=\"risk-orchestrator\"}[5m]))",
          "legendFormat": "{{calculationType}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 2,
      "title": "VaR Calculation Duration (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(var_calculation_duration_seconds_bucket{job=\"risk-orchestrator\"}[5m]))",
          "legendFormat": "p95",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.50, rate(var_calculation_duration_seconds_bucket{job=\"risk-orchestrator\"}[5m]))",
          "legendFormat": "p50",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 3,
      "title": "Request Rate by Route",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (route) (rate(ktor_http_server_requests_seconds_count{job=\"risk-orchestrator\"}[5m]))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 4,
      "title": "Error Rate (5xx)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (route) (rate(ktor_http_server_requests_seconds_count{job=\"risk-orchestrator\",status=~\"5..\"}[5m]))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
