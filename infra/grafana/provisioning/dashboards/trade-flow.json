{
  "uid": "kinetix-trade-flow",
  "title": "Trade Flow",
  "tags": ["kinetix", "trading"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Trade Booking Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(ktor_http_server_requests_seconds_count{job=\"position-service\", uri=~\".*trades.*\", method=\"POST\"}[5m])",
          "legendFormat": "Trades booked/s"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Trade Booking Latency (P95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ktor_http_server_requests_seconds_bucket{job=\"position-service\", uri=~\".*trades.*\", method=\"POST\"}[5m]))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.50, rate(ktor_http_server_requests_seconds_bucket{job=\"position-service\", uri=~\".*trades.*\", method=\"POST\"}[5m]))",
          "legendFormat": "P50"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Trade Error Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "rate(ktor_http_server_requests_seconds_count{job=\"position-service\", uri=~\".*trades.*\", status=~\"4..|5..\"}[5m])",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Price Update Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "rate(price_updates_total{job=\"price-service\"}[5m])",
          "legendFormat": "Price updates/s"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Price Feed Ingestion Latency",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(price_feed_latency_seconds_bucket{job=\"price-service\"}[5m]))",
          "legendFormat": "P95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Gateway Request Rate by Route",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "targets": [
        {
          "expr": "sum by (uri) (rate(ktor_http_server_requests_seconds_count{job=\"gateway\"}[5m]))",
          "legendFormat": "{{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Kafka Trade Event Lag",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "kafka_consumer_records_lag{topic=\"trades.lifecycle\"}",
          "legendFormat": "{{group}} partition={{partition}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "title": "Risk Orchestrator Request Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "sum by (uri) (rate(ktor_http_server_requests_seconds_count{job=\"risk-orchestrator\"}[5m]))",
          "legendFormat": "{{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}
