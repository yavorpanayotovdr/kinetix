{
  "uid": "kinetix-service-overview",
  "title": "Service Overview",
  "tags": ["kinetix", "overview"],
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [
    { "title": "System Health", "type": "link", "url": "/d/kinetix-system-health", "icon": "dashboard" },
    { "title": "Service Overview", "type": "link", "url": "/d/kinetix-service-overview", "icon": "dashboard" },
    { "title": "Trade Flow", "type": "link", "url": "/d/kinetix-trade-flow", "icon": "dashboard" },
    { "title": "Risk Engine", "type": "link", "url": "/d/kinetix-risk-engine", "icon": "dashboard" },
    { "title": "Kafka Health", "type": "link", "url": "/d/kinetix-kafka-health", "icon": "dashboard" },
    { "title": "Database Health", "type": "link", "url": "/d/kinetix-database-health", "icon": "dashboard" },
    { "title": "Service Logs", "type": "link", "url": "/d/kinetix-service-logs", "icon": "dashboard" }
  ],
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "query": "label_values(ktor_http_server_requests_seconds_count, job)",
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "current": {
          "text": "gateway",
          "value": "gateway"
        },
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Request Rate by Route",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (route) (rate(ktor_http_server_requests_seconds_count{job=\"$service\"}[5m]))",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "Error Rate (4xx + 5xx)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum by (status) (rate(ktor_http_server_requests_seconds_count{job=\"$service\", status=~\"[45]..\"}[5m]))",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "P95 Latency by Route",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (route, le) (rate(ktor_http_server_requests_seconds_bucket{job=\"$service\"}[5m])))",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "JVM Memory Used",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{job=\"$service\"}",
          "legendFormat": "{{area}} {{id}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" },
        "overrides": []
      }
    },
    {
      "id": 5,
      "title": "JVM Thread Count",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "jvm_threads_live_threads{job=\"$service\"}",
          "legendFormat": "Live threads"
        },
        {
          "expr": "jvm_threads_daemon_threads{job=\"$service\"}",
          "legendFormat": "Daemon threads"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "GC Pause Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{job=\"$service\"}[5m])",
          "legendFormat": "{{cause}} {{action}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}
